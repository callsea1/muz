<h1>Listing song_rooms</h1>

<table>
  <tr>
    <th>User</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @song_rooms.each do |song_room| %>
  <tr>
    <td><%= song_room.user_id %></td>
    <td><%= song_room.description %></td>
    <td><%= link_to 'Show', song_room %></td>
    <td><%= link_to 'Edit', edit_song_room_path(song_room) %></td>
    <td><%= link_to 'Destroy', song_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

